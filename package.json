{
  "name": "calendz-api-calendar",
  "version": "1.0.0",
  "description": "API wrapper for EPSI's agenda",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "dev": "./node_modules/.bin/nodemon ./src/app.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha ./src/tests --recursive --timeout 15000 --exit",
    "test:coverage": "NODE_ENV=test nyc --reporter=html --reporter=text -x '**/tests/*' ./node_modules/.bin/mocha ./src/tests --recursive --timeout 15000 --exit",
    "test:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "./node_modules/.bin/eslint ./",
    "lint:fix": "./node_modules/.bin/eslint ./ --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run lint"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calendz/calendz-api-calendar.git"
  },
  "keywords": [],
  "author": "calendz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/calendz/calendz-api-calendar/issues"
  },
  "homepage": "https://github.com/calendz/calendz-api-calendar#readme",
  "dependencies": {
    "bluebird": "^3.7.1",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "convict": "^5.0.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-async-handler": "^1.1.4",
    "faker": "^4.1.0",
    "helmet": "^3.15.1",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "node-cron": "^2.0.3",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.4",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.4",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.10",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  }
}
