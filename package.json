{
  "name": "calendz-api-calendar",
  "description": "API wrapper for EPSI's agenda",
  "author": "Calendz",
  "license": "ISC",
  "private": true,
  "version": "3.0.0-SNAPSHOT",
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "test": "node ace test",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "calendz",
    "calendz-api-calendar"
  ],
  "homepage": "https://github.com/calendz/calendz-api-calendar#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calendz/calendz-api-calendar.git"
  },
  "bugs": {
    "url": "https://github.com/calendz/calendz-api-calendar/issues"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.5.1",
    "@types/proxy-addr": "^2.0.0",
    "@types/request": "^2.48.8",
    "@types/source-map-support": "^0.5.7",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^5.0.0",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.2",
    "typescript": "~4.6",
    "youch": "^3.2.3",
    "youch-terminal": "^2.2.2"
  },
  "dependencies": {
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/redis": "^7.3.3",
    "@adonisjs/repl": "^3.1.11",
    "cheerio": "^1.0.0-rc.12",
    "ioredis": "^5.3.2",
    "moment": "^2.29.4",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "source-map-support": "^0.5.21"
  }
}
